<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape>
            <corners android:radius="2dip" />
            <stroke android:color="@color/prosper_white"  android:width="2dp"/>
         </shape>
    </item>

    <item
        android:id="@android:id/progress"
        >
        <clip>
            <shape>
                <corners
                    android:radius="2dip" />
                <gradient
                    android:startColor="@color/prosper_white"
                    android:endColor="@color/prosper_white"
                    android:angle="0" />
            </shape>
        </clip>
    </item>

</layer-list>